u0430\u043d\u0438\u044e \u043f\u043e\u043c\u043e\u0449\u0438 \u0436\u0435\u043d\u0449\u0438\u043d\u0430\u043c \u0438 \u0434\u0435\u0442\u044f\u043c, \u043f\u043e\u0434\u0432\u0435\u0440\u0433\u0430\u044e\u0449\u0438\u043c\u0441\u044f \u043d\u0430\u0441\u0438\u043b\u0438\u044e.<\/p>\n<p><img loading=\"lazy\" decoding=\"async\" title=\"\u0427\u0442\u043e\u0431\u044b \u0443\u0432\u0435\u043b\u0438\u0447\u0438\u0442\u044c \u043a\u043b\u0438\u043a\u043d\u0438\u0442\u0435\" src=\"https:\/\/iswc.ru\/images\/crizis\/criz12.jpg\" alt=\"\u0427\u0442\u043e\u0431\u044b \u0443\u0432\u0435\u043b\u0438\u0447\u0438\u0442\u044c \u043a\u043b\u0438\u043a\u043d\u0438\u0442\u0435\" width=\"380\" height=\"270\" name=\"Click19\" align=\"center\" border=\"2\" hspace=\"30\" vspace=\"0\" \/><\/p>\n<p><img loading=\"lazy\" decoding=\"async\" title=\"\u0427\u0442\u043e\u0431\u044b \u0443\u0432\u0435\u043b\u0438\u0447\u0438\u0442\u044c \u043a\u043b\u0438\u043a\u043d\u0438\u0442\u0435\" src=\"https:\/\/iswc.ru\/images\/crizis\/criz13.jpg\" alt=\"\u0427\u0442\u043e\u0431\u044b \u0443\u0432\u0435\u043b\u0438\u0447\u0438\u0442\u044c \u043a\u043b\u0438\u043a\u043d\u0438\u0442\u0435\" width=\"380\" height=\"270\" name=\"Click20\" align=\"center\" border=\"2\" hspace=\"30\" vspace=\"0\" \/><\/p>\n<p>&nbsp;<\/p>\n<p><b>2009 &#8212; 2010 \u0433.\u0433.<\/b> &#8212; \u043f\u0440\u043e\u0435\u043a\u0442 &#171;\u0421\u043e\u0432\u0435\u0440\u0448\u0435\u043d\u0441\u0442\u0432\u043e\u0432\u0430\u043d\u0438\u0435 \u0441\u0438\u0441\u0442\u0435\u043c\u044b \u0440\u0435\u0430\u0433\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u043d\u0430 \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u0443 \u043d\u0430\u0441\u0438\u043b\u0438\u044f \u0432 \u043e\u0442\u043d\u043e\u0448\u0435\u043d\u0438\u0438 \u0436\u0435\u043d\u0449\u0438\u043d, \u0440\u0430\u0437\u0432\u0438\u0442\u0438\u0435 \u0441\u043e\u0437\u0434\u0430\u043d\u043d\u043e\u0439 \u0440\u0435\u0433\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u043e\u0439 \u0441\u0435\u0442\u0438 \u0426\u0435\u043d\u0442\u0440\u0430 \u0432 \u043e\u0431\u043b\u0430\u0441\u0442\u0438&#187;. \u041f\u0440\u043e\u0435\u043a\u0442 \u0431\u044b\u043b \u0440\u0435\u0430\u043b\u0438\u0437\u043e\u0432\u0430\u043d \u0441\u043e\u0432\u043c\u0435\u0441\u0442\u043d\u043e \u0441 \u041d\u0430\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u044b\u043c \u0446\u0435\u043d\u0442\u0440\u043e\u043c \u043f\u043e \u043f\u0440\u0435\u0434\u043e\u0442\u0432\u0440\u0430\u0449\u0435\u043d\u0438\u044e \u043d\u0430\u0441\u0438\u043b\u0438\u044f &#171;\u0410\u041d\u041d\u0410&#187; \u043f\u0440\u0438 \u043f\u043e\u0434\u0434\u0435\u0440\u0436\u043a\u0435 \u0424\u043e\u043d\u0434\u0430 \u0424\u043e\u0440\u0434\u0430. \u0412 \u0440\u0430\u043c\u043a\u0430\u0445 \u043f\u0440\u043e\u0435\u043a\u0442\u0430 \u0431\u044b\u043b \u043f\u043e\u0434\u0433\u043e\u0442\u043e\u0432\u043b\u0435\u043d \u0440\u0435\u0433\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u044b\u0439 \u0434\u043e\u043a\u043b\u0430\u0434 \u043f\u043e \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u0435 \u043d\u0430\u0441\u0438\u043b\u0438\u044f \u0432 \u043e\u0442\u043d\u043e\u0448\u0435\u043d\u0438\u0438 \u0436\u0435\u043d\u0449\u0438\u043d, \u043f\u0440\u043e\u0432\u0435\u0434\u0435\u043d\u044b \u043a\u0440\u0443\u0433\u043b\u044b\u0435 \u0441\u0442\u043e\u043b\u044b \u043f\u043e \u0442\u0435\u043c\u0435 \u00ab\u0421\u0443\u0434\u0435\u0431\u043d\u043e-\u043f\u0440\u0430\u0432\u043e\u0432\u0430\u044f \u043f\u0440\u0430\u043a\u0442\u0438\u043a\u0430 \u0438 \u0432\u043e\u043f\u0440\u043e\u0441\u044b \u043f\u0440\u043e\u0444\u0438\u043b\u0430\u043a\u0442\u0438\u043a\u0438 \u043f\u0440\u0435\u0434\u043e\u0442\u0432\u0440\u0430\u0449\u0435\u043d\u0438\u044f \u043d\u0430\u0441\u0438\u043b\u0438\u044f \u0432 \u0441\u0435\u043c\u044c\u0435\u00bb \u0438 \u00ab\u0420\u043e\u043b\u044c \u043e\u0431\u0449\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0441\u0442\u0438 \u0432 \u0432\u043e\u043f\u0440\u043e\u0441\u0430\u0445 \u0437\u0430\u0449\u0438\u0442\u044b \u043f\u0440\u0430\u0432 \u043f\u043e\u0442\u0435\u0440\u043f\u0435\u0432\u0448\u0438\u0445\u00bb.<\/p>\n<p><img loading=\"lazy\" decoding=\"async\" title=\"\u0427\u0442\u043e\u0431\u044b \u0443\u0432\u0435\u043b\u0438\u0447\u0438\u0442\u044c \u043a\u043b\u0438\u043a\u043d\u0438\u0442\u0435\" src=\"https:\/\/iswc.ru\/images\/crizis\/criz14.jpg\" alt=\"\u0427\u0442\u043e\u0431\u044b \u0443\u0432\u0435\u043b\u0438\u0447\u0438\u0442\u044c \u043a\u043b\u0438\u043a\u043d\u0438\u0442\u0435\" width=\"380\" height=\"270\" name=\"Click21\" align=\"center\" border=\"2\" hspace=\"30\" vspace=\"0\" \/><\/p>\n<p><b>\u041c\u0430\u0439, 2010 \u0433. <\/b>&#8212; \u0432\u044b\u0435\u0437\u0434\u043d\u043e\u0435 \u0437\u0430\u0441\u0435\u0434\u0430\u043d\u0438\u0435 \u041e\u0431\u0449\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0433\u043e \u0441\u043e\u0432\u0435\u0442\u0430 \u043f\u0440\u0438 \u0423\u0412\u0414 \u043f\u043e \u041f\u0441\u043a\u043e\u0432\u0441\u043a\u043e\u0439 \u043e\u0431\u043b\u0430\u0441\u0442\u0438. \u0412 \u0440\u0430\u043c\u043a\u0430\u0445 \u0434\u0430\u043d\u043d\u043e\u0433\u043e \u0437\u0430\u0441\u0435\u0434\u0430\u043d\u0438\u044f \u043d\u0430 \u0431\u0430\u0437\u0435 \u043d\u0430\u0448\u0435\u0433\u043e \u0426\u0435\u043d\u0442\u0440\u0430 \u043f\u0440\u043e\u0448\u043b\u0430 \u0430\u043a\u0446\u0438\u044f \u00ab\u0417\u0430\u0434\u0430\u0439 \u0432\u043e\u043f\u0440\u043e\u0441 \u0443\u0447\u0430\u0441\u0442\u043a\u043e\u0432\u043e\u043c\u0443\u00bb.<\/p>\n<p><img loading=\"lazy\" decoding=\"async\" title=\"\u0427\u0442\u043e\u0431\u044b \u0443\u0432\u0435\u043b\u0438\u0447\u0438\u0442\u044c \u043a\u043b\u0438\u043a\u043d\u0438\u0442\u0435\" src=\"https:\/\/iswc.ru\/images\/crizis\/criz15.jpg\" alt=\"\u0427\u0442\u043e\u0431\u044b \u0443\u0432\u0435\u043b\u0438\u0447\u0438\u0442\u044c \u043a\u043b\u0438\u043a\u043d\u0438\u0442\u0435\" width=\"250\" height=\"380\" name=\"Click22\" align=\"center\" border=\"2\" hspace=\"30\" vspace=\"0\" \/><\/p>\n<p>&nbsp;<\/p>\n<p><b>\u041d\u043e\u044f\u0431\u0440\u044c, 2010 \u0433.<\/b> \u2013 \u0441\u043e\u0432\u043c\u0435\u0441\u0442\u043d\u043e \u0441 \u041a\u043e\u043c\u0438\u0442\u0435\u0442\u043e\u043c \u0413\u0440\u0430\u0436\u0434\u0430\u043d\u0441\u043a\u043e\u0433\u043e \u041a\u043e\u043d\u0442\u0440\u043e\u043b\u044f. \u041f\u0440\u043e\u0432\u0435\u0434\u0435\u043d \u041a\u0440\u0443\u0433\u043b\u044b\u0439 \u0441\u0442\u043e\u043b: \u00ab\u041f\u0440\u043e\u0444\u0438\u043b\u0430\u043a\u0442\u0438\u043a\u0430 \u0438 \u043f\u0440\u0435\u0434\u0443\u043f\u0440\u0435\u0436\u0434\u0435\u043d\u0438\u0435 \u043d\u0430\u0441\u0438\u043b\u0438\u044f \u0432 \u0441\u0435\u043c\u044c\u0435 \u0438 \u043e\u0431\u0449\u0435\u0441\u0442\u0432\u0435 \u043d\u0430 \u0442\u0435\u0440\u0440\u0438\u0442\u043e\u0440\u0438\u0438 \u041f\u0441\u043a\u043e\u0432\u0441\u043a\u043e\u0439 \u043e\u0431\u043b\u0430\u0441\u0442\u0438\u00bb. \u0426\u0435\u043b\u044c\u044e \u043c\u0435\u0440\u043e\u043f\u0440\u0438\u044f\u0442\u0438\u044f \u0431\u044b\u043b\u043e \u0441\u0444\u043e\u0440\u043c\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u0440\u0430\u0431\u043e\u0447\u0443\u044e \u0433\u0440\u0443\u043f\u043f\u0443 \u043f\u043e \u0440\u0430\u0431\u043e\u0442\u0435 \u043d\u0430\u0434 \u043f\u0440\u043e\u0435\u043a\u0442\u043e\u043c \u0446\u0435\u043b\u0435\u0432\u043e\u0439 \u043c\u0435\u0436\u0432\u0435\u0434\u043e\u043c\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0439 \u043e\u0431\u043b\u0430\u0441\u0442\u043d\u043e\u0439 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u044b \u043f\u043e \u043f\u0440\u043e\u0444\u0438\u043b\u0430\u043a\u0442\u0438\u043a\u0435 \u0438 \u043f\u0440\u0435\u0434\u0443\u043f\u0440\u0435\u0436\u0434\u0435\u043d\u0438\u044e \u043d\u0430\u0441\u0438\u043b\u0438\u044f \u0432 \u0441\u0435\u043c\u044c\u0435 \u0438 \u043e\u0431\u0449\u0435\u0441\u0442\u0432\u0435.<\/p>\n<p><img loading=\"lazy\" decoding=\"async\" title=\"\u0427\u0442\u0